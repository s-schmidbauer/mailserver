{% if ipv4_autoconf == true %}
dhcp
{% endif %}
{% if ipv4_autoconf == false %}
inet {{ ipv4_addr }} {{ ipv4_netmask }}
{% endif %}
{% if ipv6_autoconf == true %}
inet6 autoconf -autoconfprivacy -soii
{% endif %}
{% if ipv6_autoconf == false %}
inet6 autoconf -autoconfprivacy -soii
inet6 alias {{ ipv6_addr }} {{ ipv6_netmask }}
{% endif %}
